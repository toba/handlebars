{
   "name": "@toba/handlebars",
   "version": "2.1.0",
   "description": "Toba Handlebars module",
   "private": false,
   "author": {
      "name": "Jason Abbott",
      "url": "http://github.com/toba"
   },
   "license": "MIT",
   "publishConfig": {
      "access": "public"
   },
   "main": "cjs/index.js",
   "module": "esm/index.js",
   "types": "esm/index.d.ts",
   "files": [
      "esm",
      "cjs"
   ],
   "scripts": {
      "build:esm": "./node_modules/.bin/tsc --project ./node_modules/@toba/develop/tsconfig.esm.json",
      "build:cjs": "./node_modules/.bin/tsc --project ./node_modules/@toba/develop/tsconfig.cjs-min.json",
      "prepublishOnly": "npm run build:esm & npm run build:cjs",
      "test": "jest --coverage"
   },
   "devDependencies": {
      "@toba/develop": "^6.6.11",
      "@toba/test": "^4.1.0",
      "@types/express": "^4.16.0",
      "@types/handlebars": "^4.0.38",
      "@types/supertest": "^2.0.4",
      "express": "^4.16.3",
      "supertest": "^4.0.0"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/toba/handlebars"
   },
   "bugs": {
      "url": "https://github.com/toba/handlebars/issues"
   },
   "dependencies": {
      "@toba/node-tools": "^1.4.5",
      "@toba/tools": "^6.0.0",
      "handlebars": "^4.0.11"
   }
}
